pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                bat '"C:\\Windows\\System32\\cmd.exe" /c echo "Building..."'
            }
        }
        stage('Test') {
            steps {
                bat '"C:\\Windows\\System32\\cmd.exe" /c echo "Testing..."'
            }
        }
        stage('Deploy') {
            steps {
                bat '"C:\\Windows\\System32\\cmd.exe" /c echo "Deploying..."'
            }
        }
    }
}
