pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                bat 'cmd /c echo "Building..."'
            }
        }
        stage('Test') {
            steps {
                bat 'cmd /c echo "Testing..."'
            }
        }
        stage('Deploy') {
            steps {
                bat 'cmd /c echo "Deploying..."'
            }
        }
    }
}
